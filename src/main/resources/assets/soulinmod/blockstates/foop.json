{
  "multipart": [
    {
      "apply": {
        "model": "soulinmod:block/foop_post"
      }
    },
    {
      "when": {
        "north": true
      },
      "apply": {
        "model": "soulinmod:block/foop_side"
      }
    },
    {
      "when": {
        "east": true
      },
      "apply": {
        "model": "soulinmod:block/foop_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": true
      },
      "apply": {
        "model": "soulinmod:block/foop_side_alt"
      }
    },
    {
      "when": {
        "west": true
      },
      "apply": {
        "model": "soulinmod:block/foop_side_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": false
      },
      "apply": {
        "model": "soulinmod:block/foop_noside"
      }
    },
    {
      "when": {
        "east": false
      },
      "apply": {
        "model": "soulinmod:block/foop_noside_alt"
      }
    },
    {
      "when": {
        "south": false
      },
      "apply": {
        "model": "soulinmod:block/foop_noside_alt",
        "y": 90
      }
    },
    {
      "when": {
        "west": false
      },
      "apply": {
        "model": "soulinmod:block/foop_noside",
        "y": 270
      }
    }
  ]
}